{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Remove a parameter (block) from a Haddock project",
  "type": "object",
  "required": [
    "project_id",
    "block_id"
  ],
  "additionalProperties": false,
  "properties": {
    "project_id": {
      "description": "Haddock project identifier",
      "type": "string"
    },
    "block_id": {
      "description": "Parameter (block) to remove. Use dot notation for nested blocks",
      "type": "string"
    },
    "multiple": {
      "description": "Remove multiple if found",
      "type": "boolean",
      "default": false
    }
  }
}